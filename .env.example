# Example environment variables for FrontBet
# Monetary values are expressed in cents
NEXT_PUBLIC_MIN_DEPOSIT=1000
NEXT_PUBLIC_MAX_DEPOSIT=1500000
NEXT_PUBLIC_MIN_WITHDRAWAL=2000

# API base for integration tests / runtime
#NEXT_PUBLIC_API_BASE_URL=https://api.example.com
## Example environment variables for frontBet

# Public URL exposed to browser builds (recommended variable name)
NEXT_PUBLIC_API_BASE_URL=https://backbet.onrender.com

# (Legacy names removed) The code reads `NEXT_PUBLIC_API_BASE_URL`.

# Request timeout in ms (0 = no timeout)
API_TIMEOUT_MS=10000

# Toggle MSW/mock usage in browser and node
NEXT_PUBLIC_USE_MOCKS=false
USE_MOCKS=false

# Clerk (example values for local development)
CLERK_PUBLISHABLE_KEY=pk_test_your_key_here
CLERK_SECRET_KEY=sk_test_your_key_here

# Frontend dev url
FRONTEND_API_URL=http://localhost:3000
# Clerk
CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
